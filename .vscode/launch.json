{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python Debugger: Current File",
			"type": "debugpy",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal",
			"justMyCode": false
		},
		{
			"cwd": "${workspaceFolder}",
			"executable": "${command:cmake.launchTargetPath}",
			"name": "Debug STM32F723 with ST-Link",
			"request": "launch",
			"type": "cortex-debug",
			"showDevDebugOutput": "none",
			"servertype": "stlink",
			"liveWatch": {
				"enabled": true,
				"samplesPerSecond": 1
			},
			"svdPath": "STM32F723.svd"
		},
		{
			"cwd": "${workspaceFolder}",
			"executable": "${command:cmake.launchTargetPath}",
			"name": "Attach STM32F723 with ST-Link",
			"request": "attach",
			"type": "cortex-debug",
			"showDevDebugOutput": "none",
			"servertype": "stlink",
			"liveWatch": {
				"enabled": true,
				"samplesPerSecond": 1
			},
			"svdPath": "STM32F723.svd"
		},
		{
			"cwd": "${workspaceFolder}",
			"executable": "${command:cmake.launchTargetPath}",
			"name": "Debug STM32N657 with ST-Link",
			"request": "launch",
			"type": "cortex-debug",
			"showDevDebugOutput": "none",
			"servertype": "stlink",
			"liveWatch": {
				"enabled": true,
				"samplesPerSecond": 1
			},
			"svdPath": "STM32N657.svd",
			"serverArgs": [
				"-m",
				"1"
			],
			"overrideLaunchCommands": [
				"set *0x56028A4C=0xFFFFFF",
				"set *0x52023080=0",
				"set *0x52023100=0",
				"set *0x52023180=0",
				"set *0x52023200=0",
				"set *0x52023280=0",
				"-target-download",
				"monitor halt\""
			]
		},
		{
			"cwd": "${workspaceFolder}",
			"executable": "${command:cmake.launchTargetPath}",
			"name": "Attach STM32N657 with ST-Link",
			"request": "attach",
			"type": "cortex-debug",
			"showDevDebugOutput": "none",
			"servertype": "stlink",
			"liveWatch": {
				"enabled": true,
				"samplesPerSecond": 1
			},
			"svdPath": "STM32N657.svd"
		}
	]
}

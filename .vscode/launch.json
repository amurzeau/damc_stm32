{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python Debugger: Current File",
			"type": "debugpy",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal",
			"justMyCode": false
		},
		{
			"cwd": "${workspaceFolder}",
			"executable": "${command:cmake.launchTargetPath}",
			"name": "Debug STM32F723 with ST-Link",
			"request": "launch",
			"type": "cortex-debug",
			"showDevDebugOutput": "none",
			"servertype": "stlink",
			"liveWatch": {
				"enabled": true,
				"samplesPerSecond": 1
			},
			"svdPath": "STM32F723.svd"
		},
		{
			"cwd": "${workspaceFolder}",
			"executable": "${command:cmake.launchTargetPath}",
			"name": "Attach STM32F723 with ST-Link",
			"request": "attach",
			"type": "cortex-debug",
			"showDevDebugOutput": "none",
			"servertype": "stlink",
			"liveWatch": {
				"enabled": true,
				"samplesPerSecond": 1
			},
			"svdPath": "STM32F723.svd"
		},
		{
			"cwd": "${workspaceFolder}",
			"executable": "${command:cmake.launchTargetPath}",
			"name": "Debug STM32N657 with ST-Link",
			"request": "launch",
			"type": "cortex-debug",
			"showDevDebugOutput": "none",
			"servertype": "stlink",
			"liveWatch": {
				"enabled": true,
				"samplesPerSecond": 1
			},
			"svdPath": "STM32N657.svd",
			"serverArgs": [
				"-m",
				"1"
			],
			"overrideLaunchCommands": [
				"-target-download",
				"-interpreter-exec console \"monitor halt\""
			]
		},
		{
			"cwd": "${workspaceFolder}",
			"executable": "${command:cmake.launchTargetPath}",
			"name": "Attach STM32N657 with ST-Link",
			"request": "attach",
			"type": "cortex-debug",
			"showDevDebugOutput": "none",
			"servertype": "stlink",
			"liveWatch": {
				"enabled": true,
				"samplesPerSecond": 1
			},
			"svdPath": "STM32N657.svd"
		}
	]
}
